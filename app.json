{
  "expo": {
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/Fonts/Sora-Bold.ttf", 
                    "./assets/Fonts/Sora-Regular.ttf", 
                    "./assets/Fonts/Sora-SemiBold.ttf"]
        }
      ]
    ],
    "name": "Budget",
    "slug": "Budget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Budget_Logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/Budget_Logo.png"
    }
    
  }
}
